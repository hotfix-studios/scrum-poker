{
  "name": "scrum-poker",
  "version": "1.0.0",
  "description": "This is an application that integrates with GitHub Projects for seamless import of product backlogs into a UI for easy sprint estimation.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "//": "production:",
    "start": "npm run build && npm run server",
    "build": "tsc -p .",
    "server": "node ./dist/app.js",
    "//": "development:",
    "wss": "tsc-watch -p tsconfig.json --onSuccess \"node ./dist/wss.js\"",
    "dev": "tsc-watch -p tsconfig.json --onSuccess \"npm run server\""
  },
  "keywords": ["agile", "devops", "scrum", "sprint", "estimation", "scrum-poker", "poker", "gamify", "pointing"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^20.11.20",
    "@types/ws": "^8.5.10",
    "dotenv": "^16.4.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.1",
    "octokit": "^3.1.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "smee-client": "^2.0.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  }
}
